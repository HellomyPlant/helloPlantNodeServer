    "eslint-plugin-import": "^2.25.4",
    "express-session": "^1.17.2",
    "express-validation": "^3.0.8",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.6",
    "mongoose-unique-validator": "^3.0.0",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.1.2",
    "swagger-cli": "^4.0.4",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0",
    "yamljs": "^0.3.0"
  }
}
